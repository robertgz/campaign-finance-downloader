{
  "name": "campaign-finance-downloader",
  "version": "0.0.0",
  "description": "Library for scraping campaign finance data from multiple sources",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "fixup": "node ./scripts/fixup",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run fixup",
    "prepare": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.15.0 <17",
    "npm": ">=8.5.5"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "playwright": "^1.23.2",
    "puppeteer": "^14.1.1",
    "ts-node": "^10.8.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/jest": "^28.1.4",
    "@types/puppeteer": "^5.4.6",
    "jest": "^28.1.2",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
